public class No {
    private int valor;
    private int left;
    private int rigth;

    public No(int valor, int left, int rigth) {
        this.valor = valor;
        this.rigth = left;
        this.left = left;
    }

    public int getValor() {
        return valor;
    }

    public int gatrigth() {
        return rigth;
    }

    public int getleft() {
        return left;
    }

    public void setValor(int valor) {
        this.valor = valor;
    }

    public void setrigth(int rigth) {
        this.rigth = rigth;
    }
    
    public void setleft(int left) {
        this.left = left;
    }
    
}